
/**
 * Description : A faire
 *
 * @author Thierry Mater
 * @version 1.0.0 (23/08/2017)
 */
public class Summary
{
    // variables d'instance

    /**
     * Déclaration des variables du modéle
     */
    private String m_nom;
    private String m_siege;
    private String m_option;
    private String m_action1;
    private String m_actionInfo1;
    private String m_action2;
    private String m_actionInfo2;
    private String m_gain;

    /**
     * Default constructor.
     */
    public Summary() {

    }

    /**
     * 
     * @param siege
     * @param option
     * @param action1
     * @param actionInfo1
     * @param action2
     * @param actionInfo2
     * @param gain
     */
    public Summary(String siege, String nom, String option, String action1, 
    String actioninfo1, String action2, String actioninfo2, String gain)
    {

        m_nom = nom;
        m_siege = siege;
        m_option = option;
        m_action1 = action1;
        m_actionInfo1 = actioninfo1;
        m_action2 = action2;
        m_actionInfo2 = actioninfo2;
        m_gain = gain;
    }

    public String getSiege()
    {
        return m_siege;
    }

    public String getNom()
    {
        return m_nom;
    }

    public String getOption() 
    {
        return m_option;
    }

    public String getAction1()
    {
        return m_action1;
    }

    public String getActionInfo1()
    {
        return m_actionInfo1;
    }

    public String getAction2()
    {
        return m_action2;
    }

    public String getActionInfo2()
    {
        return m_actionInfo2;
    }

    public String getGain()
    {
        return m_gain;
    }
}
